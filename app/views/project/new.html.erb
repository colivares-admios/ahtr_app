<div class="authform_large">
   <%= form_for(:project, url: project_index_path,  :method => "post",:html => { :role => 'form', :multipart => true}) do |f| %>


   
    <div class="col-md-12">
    	<%= devise_error_messages! %>
       <h3>Create a New Project</h3>
    </div>
  <div class="row">
<div class="col-md-6">
          <div class="form-group">
            <%= f.label :name %>
            <%= f.text_field :name, :autofocus => true, class: 'form-control' %>
          </div>
           <div class="form-group">
            <%= f.label :description %>
            <%= f.text_area :description, rows: 8, class: 'form-control' %>
          </div>


         
    </div>
    <div class="col-md-6">
    	   <div class="form-group">
            <%= f.label :Start_Date %>
            <%= f.date_field :start_date, class: 'form-control' %>
          </div>
          <div class="form-group">
            <%= f.label :Finish_Date %>
            <%= f.date_field :finish_date, class: 'form-control' %>
          </div>
          <div class="form-group">
            <%= f.label :Website %>
            <%= f.url_field :website, class: 'form-control' %>
          </div>
          <div class="form-group">
            <%= f.label :Github_Repository %>
            <%= f.url_field :github, class: 'form-control' %>
          </div>


 
    </div>
    <div class="col-md-12">
      <%= f.submit 'Create Project', :class => 'btn btn-primary btn-lg btn-block' %>
    </div>
  </div>
  <% end %>
</div>
